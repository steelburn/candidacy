APP_NAME=AI-Service
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=candidacy_ai
DB_USERNAME=root
DB_PASSWORD=root

# AI Provider Configuration
AI_PROVIDER=ollama
# Options: ollama, openrouter

# Ollama Configuration
OLLAMA_URL=http://ollama:11434
OLLAMA_MODEL=mistral

# OpenRouter Configuration (get key from https://openrouter.ai/)
OPENROUTER_API_KEY=
OPENROUTER_MODEL=mistralai/mistral-7b-instruct
# Other models: openai/gpt-3.5-turbo, anthropic/claude-2, etc.

CACHE_DRIVER=file
QUEUE_CONNECTION=sync

REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379
